<!DOCTYPE html>

<html lang="en">
    <head>
        <link href="https://ajax.googleapis.com/ajax/libs/jqueryui/1.9.1/themes/base/jquery-ui.css" rel="stylesheet" type="text/css"/>
        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
        <script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.9.1/jquery-ui.min.js"></script>
        
        <link rel="stylesheet" href="${request.static_url('bob:static/css/bootstrap.min.css')}" type="text/css" media="screen" charset="utf-8" />
        <script src="${request.static_url('bob:static/js/bootstrap.min.js')}"></script>
        <script src="${request.static_url('bob:static/js/jcanvas.min.js')}"></script>
        <script src="${request.static_url('bob:static/js/gomoku/visualizer.min.js')}"></script>
        <script>
            visualize("${replay_json_url}", 16);
        </script>
    </head>
    <body>
        <div class="container" style="width: 680px">
                <canvas id="game-canvas" width="640" height="640"></canvas>

                <br/>
                <span id="game-current-turn"></span>/<span id="game-maximum-turn"></span>
                <div class="btn-group">
                    <button class="btn" id="game-button-previous"><i class="icon-arrow-left"></i></button>
                    <button class="btn" data-toggle="button" id="game-button-play-pause"><i id="game-icon-play-pause" class="icon-play"></i></button>
                    <button class="btn" id="game-button-next"><i class="icon-arrow-right"></i></button>
                </div>
        </div>
    </body>
</html>

